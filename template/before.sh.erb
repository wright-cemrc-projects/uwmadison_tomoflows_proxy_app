#!/bin/bash

[[ $(type -t module) == "function" ]] && export -f module

# Restrict the port range
backend_port=$(find_port ${host} 5000 6000)
frontend_port=$(find_port ${host} 5000 6000)

# Export both ports to be used by TomoFlows in startup
export TF_BACKEND_PORT=$backend_port
export TF_FRONTEND_PORT=$frontend_port

